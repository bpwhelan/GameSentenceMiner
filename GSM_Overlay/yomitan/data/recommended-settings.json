                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": false
                    },
                    {
                        "enabled": false,
                        "character1": "'",
                        "character2": "'",
                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": false
                    },
                    {
                        "enabled": true,
                        "character1": ".",
                        "character2": null,
                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": true
                    },
                    {
                        "enabled": true,
                        "character1": "!",
                        "character2": null,
                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": true
                    },
                    {
                        "enabled": true,
                        "character1": "?",
                        "character2": null,
                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": true
                    },
                    {
                        "enabled": true,
                        "character1": "…",
                        "character2": null,
                        "includeCharacterAtStart": false,
                        "includeCharacterAtEnd": true
                    }
                ]
            },
            "description": "Disable apostrophes as a sentence terminator."
        }
